{
  "name": "ddrv/slim-app",
  "version": "2.0.0",
  "type": "project",
  "description": "Simple web application based on slim/slim and symfony/console",
  "keywords": ["php", "application", "slim", "symfony-console", "skeleton"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ivan Dudarev",
      "email": "ivan@ddrv.ru",
      "homepage": "https://ddrv.ru"
    }
  ],
  "require": {
    "php": ">=5.6",
    "slim/slim": "^3.12",
    "twig/twig": "~1.36",
    "symfony/console": "~3.4",
    "symfony/process": "~3.4",
    "analog/analog": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\App\\": "tests"
    }
  }
}

